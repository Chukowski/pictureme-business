# fal.ai API Configuration
# Get your API key from: https://fal.ai/dashboard/keys
# Important: Add both VITE_FAL_KEY (frontend) and FAL_KEY (backend)
VITE_FAL_KEY=your-fal-key-here
FAL_KEY=your-fal-key-here

# Choose AI Model:
# Option 1: fal-ai/bytedance/seedream/v4/edit (High quality, cinematic, multi-image compositing)
# Option 2: fal-ai/gemini-25-flash-image/edit (Fast, lightweight photo blending)
VITE_FAL_MODEL=fal-ai/bytedance/seedream/v4/edit

# Base URL for sharing (optional)
VITE_BASE_URL=http://localhost:8080

# Backend API URL
VITE_API_URL=http://localhost:3001

# Better Auth Configuration
BETTER_AUTH_SECRET=your-better-auth-secret
BETTER_AUTH_URL=http://localhost:8080
VITE_AUTH_URL=http://localhost:3002
DATABASE_URL=postgresql://user:password@host:port/database
AUTH_PORT=3002

# Stripe Configuration
STRIPE_SECRET_KEY=sk_live_your_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_publishable_key_here

# Email Configuration (AWS SES via SMTP)
SMTP_HOST=email-smtp.us-east-1.amazonaws.com
SMTP_PORT=587
SMTP_USERNAME=your-ses-smtp-username
SMTP_PASSWORD=your-ses-smtp-password
SMTP_FROM_EMAIL=share@yourdomain.com
SMTP_FROM_NAME=PictureMe.Now
SMTP_USE_TLS=true

